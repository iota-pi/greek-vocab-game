{
  "name": "greek-vocab",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "upload": "aws s3 cp build/ s3://greek-parsing-app-production/ --recursive --acl public-read",
    "deploy:app": "yarn build && yarn upload",
    "build:lambda": "esbuild lambda/index.ts --outfile=lambda/build/lambda.js --bundle --minify --platform=node --target=node14 --external:aws-sdk --external:canvas",
    "bundle:lambda": "cd lambda/build && 7z a -tzip lambda.zip lambda.js",
    "upload:lambda": "aws s3 cp lambda/build/lambda.zip s3://crosscode-lambdas/greek/lambda.zip",
    "deploy:lambda": "yarn build:lambda && yarn bundle:lambda && yarn upload:lambda",
    "deploy": "yarn deploy:app && yarn deploy:lambda && cd infra && terraform apply",
    "lint": "yarn run eslint src lambda --ext .ts,.tsx",
    "start": "vite",
    "serve": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
